@page "/loggedOut"

<PageTitle>A felhasználó kilépett</PageTitle>

@* A kiléptetett felhasználó értesítésére szolgáló felület. *@

<MudGrid Justify="Justify.Center">
    <MudItem xs="12" sm="12" md="12" lg="12" xl="12">
        <MudCard Style="padding-top:10%; padding-bottom:10%; background-color: rgb(255,255,255,0.5);">
            <MudCardContent>
                <MudText Align="Align.Center" Typo="Typo.h3">Sikeresen kilépett a rendszerből!</MudText>
                <MudText Align="Align.Center" Typo="Typo.h3">További kellemes napot kívánunk!</MudText>
            </MudCardContent>
        </MudCard>
    </MudItem>
</MudGrid>



